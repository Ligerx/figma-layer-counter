{
  "compilerOptions": {
    "target": "es6",
    "outDir": "dist",
    "jsx": "react",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "lib": ["DOM", "ES6", "DOM.Iterable", "ScriptHost", "ESNext.Array"]
  }
}
